# Example terraform variables file
# Copy this file to terraform.tfvars and fill in your actual values
# terraform.tfvars is ignored by git for security

# Get your Google Gemini API key from: https://aistudio.google.com/app/api-keys
gemini_api_key = "your-gemini-api-key-here"

# Your public IP address for SSH access (restricts SSH to only your IP)
allowed_ssh_cidr = "167.62.238.65/32"

# AWS SSH key pair name
key_name = "docusaurus-key"

# Database password for RDS PostgreSQL
db_password = "ChangeThisPassword123!"

# Email for infrastructure notifications
notification_email = "exampleEmail@gmail.com"